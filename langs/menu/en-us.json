	{"key": "help.default", "value": "Hover over or long-press (mobile) any setting to get information about it."},
	{"key": "help.clear_custom_routes", "value": "Clears all the custom routes currently on the map."},
	{"key": "help.clear_inventory", "value": "Clears all the items in the inventory."},
	{"key": "help.clear_markers", "value": "Resets all collected markers to be uncollected."},
	{"key": "help.cookie_export", "value": "Exports your settings to a file."},
	{"key": "help.cookie_import", "value": "Imports a settings file and sets all settings to those settings."},
	{"key": "help.cookie_import_file", "value": "Opens a file browse dialog where you can select a settings file."},
	{"key": "help.donate", "value": "Donations are appreciated and help keep the map and its development going!"},
	{"key": "help.enable_custom_route", "value": "Enables the creation mode for custom routes. Click a marker then click another one to draw a line between them."},
	{"key": "help.enable_dclick_zoom", "value": "Toggles whether the map should zoom when you double-click anywhere on it."},
	{"key": "help.enable_inventory", "value": "Toggles whether the inventory feature should be enabled or disabled."},
	{"key": "help.enable_inventory_menu_update", "value": "Toggles whether the inventory should update when you click an item in the menu. Adds or removes the items currently on the map to the inventory if enabled."},
	{"key": "help.enable_inventory_popups", "value": "Toggles whether the inventory increment and decrement buttons should be visible in a marker popup."},
	{"key": "help.enable_marker_popups", "value": "Toggles whether a popup should show when you click a marker. If disabled, the marker gets set as collected or uncollected."},
	{"key": "help.export_route", "value": "Exports the created route to your clipboard."},
	{"key": "help.generate_route", "value": "Starts the generation of a new route using the settings you specified."},
	{"key": "help.generate_route_allow_fasttravel", "value": "Toggles whether fast travel points should be considered when generating a route."},
	{"key": "help.generate_route_auto_update", "value": "Toggles whether the route should be automatically updated when you collect an item or toggle a menu item."},
	{"key": "help.generate_route_clear", "value": "Clears the currently generated route."},
	{"key": "help.generate_route_distance", "value": "Allows you to set the maximum distance any given path will be. If you set this too low, the path might not complete. If you set this too high, impossible paths could be generated."},
	{"key": "help.generate_route_generate_on_visit", "value": "Toggles whether a new route should be created when you browse to this page."},
	{"key": "help.generate_route_ignore_collected", "value": "Toggles whether a route should ignore items you've already collected."},
	{"key": "help.generate_route_start", "value": "Allows you to set where the route will start from."},
	{"key": "help.generate_route_start_custom", "value": "Allows you to set the custom coordinates where the route will start from."},
	{"key": "help.generate_route_use_pathfinder", "value": "Toggles whether or not to use the advanced path generator that follows the roads. This option has a significant impact on performance."},
	{"key": "help.hide_all", "value": "Hides all markers from the map."},
	{"key": "help.import_route", "value": "Imports a custom route to the map."},
	{"key": "help.inventory_stack", "value": "Allows you to set the maximum size of any item stack. The default is 10 which is the same as in-game."},
	{"key": "help.item_category", "value": "Toggles whether this set should be visible on the map."},
	{"key": "help.item_category_encouters", "value": "Opens a list with all random encounters."},
	{"key": "help.item_manual_cycle", "value": "Allows you to manually set the cycle that's displayed on the map."},
	{"key": "help.item_reset", "value": "Resets all items in this category to be uncollected."},
	{"key": "help.item_sell", "value": "Removes 1 from every item in the current category, useful when you've just sold this set."},
	{"key": "help.item_value", "value": "This is the current in-game value for selling a complete set."},
	{"key": "help.language", "value": "Allows you to change the language the map is displayed in."},
	{"key": "help.marker_cluster", "value": "Toggles whether markers should move out of the way when they are close together to let you select a specific marker more easily."},
	{"key": "help.pins_import_file", "value": "Imports a pins file and displays all imported pins on the map."},
	{"key": "help.pins_place_new", "value": "Creates a new pin in the center of the currently display map."},
	{"key": "help.rdo_map", "value": "Opens a new tab with the other map by Jean, where you can browse markers other than those related to the Collector role."},
	{"key": "help.read_wiki", "value": "Opens a new tab with the wiki for this map, in case you need even more information."},
	{"key": "help.report_issues", "value": "Opens a new tab to the GitHub page, where you can report issues or suggest new features."},
	{"key": "help.reset_button_updates_inventory", "value": "Toggles whether the reset button in sets remove items from the inventory too."},
	{"key": "help.reset_markers_daily", "value": "Toggles whether the map should automatically reset all markers for you when the day switches."},
	{"key": "help.return_legacy", "value": "Opens a new tab with the legacy version of this map. Note that this version is no longer updated and has a high chance of being incorrect."},
	{"key": "help.search", "value": "Allows you to search for items you want to display. All other items will be hidden."},
	{"key": "help.show_all", "value": "Shows all markers on the map."},
	{"key": "help.show_coords", "value": "Toggles whether the coordinates should be displayed when you click somewhere on the map."},
	{"key": "help.show_help", "value": "Toggles whether this help dialog should appear when you hover over or long-press (mobile) a setting."},
	{"key": "help.tool_type", "value": "Allows you to filter items based on the tools you have."},
	{"key": "help.user_pins_edit_mode", "value": "Toggles whether pins should be editable, movable and removable."},
	{"key": "help.user_pins_export", "value": "Exports your pins to a file."},
	{"key": "help.user_pins_import", "value": "Imports a pins file and displays all those pins on the map."},
	{"key": "help.user_pins_place_mode", "value": "Toggles whether a new pin should be created when you click somewhere on the map."},
	{"key": "help.marker_opacity", "value": "Allows you to change the opacity of the collectible markers on the map."},
	{"key": "help.overlay_opacity", "value": "Allows you to change the opacity of the different overlays that appear where caves, buildings, etc. are located."},
	{"key": "help.toggle_debug", "value": "Opens the debug settings which enables certain developer functionality. Using them isn't recommended for regular users."}
]
