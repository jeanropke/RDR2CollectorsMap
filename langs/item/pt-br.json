	{"key": "cups_ace.name", "value": "Ás de Copas"},
	{"key": "cups_ace_1.desc", "value": "Dentro do celeiro em cima dos fardos de feno."},
	{"key": "cups_ace_2.desc", "value": "Em um barril atrás do galpão."},
	{"key": "cups_ace_3.desc", "value": "Na mesa de cabeceira no quarto da direita."},
	{"key": "cups_ace_4.desc", "value": ""},
	{"key": "cups_ace_5.desc", "value": "Em cima do cinema."},
	{"key": "cups_ace_6.desc", "value": "No parapeito no lado leste, na frente do barco."},
	
	{"key": "cups_two.name", "value": "Dois de Copas"},
	{"key": "cups_two_1.desc", "value": "Em um barril debaixo do prédio."},
	{"key": "cups_two_2.desc", "value": "Em uma caixa na parte de baixo da ponte ferroviária."},
	{"key": "cups_two_3.desc", "value": "Na mesa de cabeceira."},
	{"key": "cups_two_4.desc", "value": ""},
	{"key": "cups_two_5.desc", "value": "No topo do moinho de vento, com vista para a cidade. Suba a escada."},
	{"key": "cups_two_6.desc", "value": "Em um barril no final das docas."},

	{"key": "cups_three.name", "value": "Três de Copas"},
	{"key": "cups_three_1.desc", "value": "Na parte de trás da carroça."},
	{"key": "cups_three_2.desc", "value": ""},
	{"key": "cups_three_3.desc", "value": "Em um banco."},
	{"key": "cups_three_4.desc", "value": "Em cima do banco na varanda da frente."},
	{"key": "cups_three_5.desc", "value": "Em uma caixa no lado leste da casa."},
	{"key": "cups_three_6.desc", "value": "Do lado de fora da cabana, na mesa que tem uma garrafa de moonshine."},

	{"key": "cups_four.name", "value": "Quatro de Copas"},
	{"key": "cups_four_1.desc", "value": "Em cima da lareira dentro do prédio."},
	{"key": "cups_four_2.desc", "value": ""},
	{"key": "cups_four_3.desc", "value": "Na janela da casa destruída."},
	{"key": "cups_four_4.desc", "value": "Em uma caixa perto de uma fonte com uma luz."},
	{"key": "cups_four_5.desc", "value": "Na varanda na parte de trás do escritório do xerife."},
	{"key": "cups_four_6.desc", "value": "No telhado do banco, na parte da frente. A escada fica atrás do banco."},

	{"key": "cups_five.name", "value": "Cinco de Copas"},
	{"key": "cups_five_1.desc", "value": "Na mesa ao lado de uma balança."},
	{"key": "cups_five_2.desc", "value": ""},
	{"key": "cups_five_3.desc", "value": "Em cima do teatro, em frente ao Blackwater Restaurant e Althewate Inn."},
	{"key": "cups_five_4.desc", "value": ""},
	{"key": "cups_five_5.desc", "value": "No banco no lado sudeste do edifício."},
	{"key": "cups_five_6.desc", "value": "Na mesa dentro do prédio, virando à esqueda na porta."},

